{
    "extends": "airbnb",
    "env": {
        "browser": true,
        "node": true,
        "es6": true,
        "jquery": true,
        "mocha": true
    },
    "rules": {
        "quotes": ["error", "double"],
        "newline-after-var": "error",
        "no-bitwise": "error",
        "no-native-reassign": "error",
        "newline-before-return": "error"
    }
}
